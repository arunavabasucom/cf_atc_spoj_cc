{
    "files.associations": {
        "*.cpp": "cpp",
        "*.h": "cpp"
    },
    "editor.cursorStyle": "line",
    "memory-view.showMemoryPanel": true,
    "cph.language.cpp.Command": "g++-14",
    "cph.general.useShortCodeForcesName": true,
    "cph.general.defaultLanguageTemplateFileLocation": "./basic_template.cpp",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "yaml": true,
        "c": false,
        "dart": false,
        "javascript": false,
        "python": false,
        "typescript": true,
        "typescriptreact": true
    }
}